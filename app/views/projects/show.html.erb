  <body>
  <body class="d-flex h-100 text-center text-dark bg-light">
    
<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
  <header class="mb-auto">
    <div>
      <h3 class="float-md-start mb-0">❖</h3>
      <nav class="nav nav-masthead justify-content-center float-md-end">
        <%= link_to "Home", root_path, class: "nav-link"%>
        <a class="nav-link" href="mailto:mousse_dental0h@icloud.com">Contact</a>
        
        <% if user_signed_in? %>
          <%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "nav-link" %>
        <% else %>
          <%= link_to "Log in", new_user_session_path, class: "nav-link"%>
        <% end %>
      </nav>
    </div>
  </header>

  <main>
    <b><p id="notice"><mark><%= notice %></mark></p></b>
    <div class="container p-5 my-5">
      <p> Image placeholder </p>
      <br></br>
      <h1><%= @project.title %></h1>
      <br></br>
      <%= @project.description %>
    </div>
  
    <% if user_signed_in? %>
    <%= link_to 'Edit', edit_project_path(@project), class: "btn btn-success"%>
    <%= link_to 'Destroy', @project, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger"%>
    <% end %>
  

  <%= link_to 'Back', projects_path, class:"btn btn-dark" %>
  <br></br>
  <br></br>



  </main>

  <footer class="mt-auto text-dark-50">
  <div class="d-inline">
    <a class="d-inline nav-link" href="https://www.linkedin.com/in/nikdelgado/">Linkedin</a> 
    <a class="d-inline nav-link" href="https://github.com/nikdelgado">Github</a>
    <a class="d-inline nav-link" href="mailto:mousse_dental0h@icloud.com">Contact</a>
    <br></br>
    <p class="text-muted"> Created by Nik Delgado &middot; &copy; 2022 </p>
  </div>
</footer>
</div>
</body>
